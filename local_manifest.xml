<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- remote -->
  <remote name="aslenofarid"
          fetch="https://github.com/aslenofarid/"/>

  <remote name="stuff"
          fetch="https://github.com/AslenoLineageStuff"/>

  <!-- device tree -->
  <project path="device/asus/X00TD"
           name="device_asus_X00TD"
           remote="aslenofarid"
           revision="lineage-17.1"/>

  <!-- common tree -->
  <project path="device/asus/sdm660-common"
           name="device_asus_sdm660-common"
           remote="aslenofarid"
           revision="lineage-17.1"/>

  <!-- vendor tree -->
  <project path="vendor/asus"
           name="proprietary_vendor_asus"
           remote="aslenofarid"
           revision="lineage-17.1"/>

  <!-- kernel tree -->
  <project path="kernel/asus/sdm660"
           name="kernel_asus_sdm660-1"
           remote="aslenofarid"
           revision="master"/>

  <!-- remove project -->
  <remove-project name="LineageOS/android_hardware_qcom_audio"/>
  <remove-project name="LineageOS/android_packages_apps_Settings"/>
  <remove-project name="LineageOS/android_frameworks_base"/>
  <remove-project name="LineageOS/android_vendor_lineage"/>
  <remove-project name="LineageOS/android_system_core"/>
  <remove-project name="LineageOS/android_build"/>

  <!-- Hardware Qualcomm CAF Repo -->
  <project path="hardware/qcom-caf/msm8998/audio" name="android_hardware_qcom_audio" remote="stuff" revision="lineage-17.1-caf-msm8998"/>

  <!-- Hals --->
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="stuff" revision="lineage-17.1"/>
  <project path="frameworks/base" name="android_frameworks_base" remote="stuff" revision="lineage-17.1"/>
  <project path="vendor/lineage" name="android_vendor_lineage" remote="stuff" revision="lineage-17.1"/>
  <project path="system/core" name="android_system_core" remote="stuff" revision="lineage-17.1"/>

  <project path="build/make" name="android_build" groups="pdk" revision="lineage-17.1" remote="stuff">
    <copyfile src="core/root.mk" dest="Makefile"/>
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk"/>
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default"/>
    <linkfile src="core" dest="build/core"/>
    <linkfile src="envsetup.sh" dest="build/envsetup.sh"/>
    <linkfile src="target" dest="build/target"/>
    <linkfile src="tools" dest="build/tools"/>
  </project>

</manifest>
