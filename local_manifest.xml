<project path="frameworks/base" name="android_frameworks_base" remote="stuff" revision="lineage-18.1"/>
<project path="package/apps/Settings" name="android_packages_apps_Settings" remote="stuff" revision="lineage-18.1"/>
<project path="system/core" name="android_system_core" remote="stuff" revision="lineage-18.1"/>
<project path="vendor/lineage" name="android_vendor_lineage" remote="stuff" revision="lineage-18.1"/>
</manifest>
