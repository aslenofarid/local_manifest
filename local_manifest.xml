<remote name="aslenofarid" 
        fetch="https://github.com/aslenofarid"/>

<remote name="stuff" 
        fetch="https://github.com/AslenoLineageStuff"/>
        
<!-- Tree -->
<project path="device/asus/X00TD" name="device_asus_X00TD" remote="aslenofarid" revision="lineage-18.1"/>
